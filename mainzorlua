local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Player = game.PLayers.LocalPlayer
local Window = OrionLib:MakeWindow({Name = "Title of the library", HidePremium = false, SaveConfig = true, IntroText = "Key System"})

OrionLib:MakeNotification({
	Name = "Logged in!",
	Content = "You are logged in as "..Player.Name..".",
	Image = "rbxassetid://4483345998",
	Time = 5
})

_G.key = "xhubuniversal2024aimbotfrl"
_G.KeyInput = "string"

function MakeScriptHub()
    loadstring(game:HttpGet(('https://raw.githubusercontent.com/ThatOneXhubUser/ghostxhubzorluahub/main/xhub')))()
end

function CorrectKeyNotification()
    OrionLib:MakeNotification({
        Name = "Correct Key Entered!",
        Content = "You have entered the correct key!",
        Image = "rbxassetid://4483345998",
        Time = 5
    })
end

function IncorrectKeyNotification()
    OrionLib:MakeNotification({
        Name = "Incorrect Key Entered!",
        Content = "You have entered the inorrect key!",
        Image = "rbxassetid://4483345998",
        Time = 5
    })
end

local Tab = Window:MakeTab({
	Name = "Key System",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddTextbox({
	Name = "Enter Key",
	Default = "Input",
	TextDisappear = true,
	Callback = function(Value)
		_G.KeyInput = Value
	end	  
})

Tab:AddButton({
	Name = "Check Key!",
	Callback = function()
      	if _G.Key == _G.Key then
        MakeScriptHub()
        CorrectKeyNotification()
        else
            IncorrectKeyNotification()
        end
  	end    
})

Tab:AddButton({
	Name = "Destroy Gui",
	Callback = function()
        OrionLib:Destroy();
  	end    
})
